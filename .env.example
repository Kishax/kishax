# aws settings
AWS_REGION=your-region
AWS_ACCOUNT_ID=your-account-id
AWS_PROFILE=your-account-profile-name

# github actions用のIAM資格情報（最小権限専用IAMユーザー）
AWS_ACCESS_KEY_ID_FOR_GITHUB_ACTIONS=your-github-actions-access-key-id
AWS_SECRET_ACCESS_KEY_FOR_GITHUB_ACTIONS=your-github-actions-secret-access-key

# cloudformation settings
AWS_STACK_NAME=your-cloudformation-stack-name

# api gateway settings
AWS_API_GATEWAY_ID=your-api-gateway-id
AWS_API_GATEWAY_RESOURCE_ID=your-api-gateway-resource-id

# lambda settings
AWS_LAMBDA_FUNCTION_NAME=kishax-infrastructure-lambda

# ecr settings
AWS_ECR_REPO_DISCORD_BOT_NAME=your-discord-bot-repo-name
AWS_ECR_REPO_GATHER_BOT_NAME=your-gather-bot-repo-name
AWS_ECR_REPO_WEB_BOT_NAME=your-web-bot-repo-name

# each markdown settings
AWS_IAM_ROLE_NAME_FOR_API_GATEWAY=your-iam-role-name-for-api-gateway
AWS_IAM_ROLE_NAME_FOR_API_GATEWAY_ACCESS_KEY=your-iam-role-access-key-for-api-gateway
AWS_IAM_POLICY_NAME_FOR_API_GATEWAY=your-iam-policy-name-for-api-gateway

AWS_VPC_ID=your-vpc-id
AWS_VPC_NAME=your-vpc-name
AWS_PRIVATE_SUBNET_IDS=your-private-subnet-id1,your-private-subnet-id2
AWS_PUBLIC_SUBNET_IDS=your-public-subnet-id1,your-public-subnet-id2

# SSL証明書設定（ACM）
AWS_SSL_CERTIFICATE_ARN=your-ssl-certificate-arn

# MySQL Configuration
MYSQL_DATABASE=mc
MYSQL_USER=mcuser
MYSQL_PASSWORD=password

# Memory Settings
SPIGOT_MEMORY=1G
VELOCITY_MEMORY=1G

# AWS Configuration
AWS_REGION=ap-northeast-1

# MC-Web SQS Communication (New dedicated user)
MC_WEB_SQS_ACCESS_KEY_ID=YOUR_MC_WEB_SQS_ACCESS_KEY_HERE
MC_WEB_SQS_SECRET_ACCESS_KEY=YOUR_MC_WEB_SQS_SECRET_KEY_HERE

# Discord API Gateway Access (Separate user)
DISCORD_API_ACCESS_KEY_ID=YOUR_DISCORD_API_ACCESS_KEY_HERE
DISCORD_API_SECRET_ACCESS_KEY=YOUR_DISCORD_API_SECRET_KEY_HERE

# SQS Queue URLs
WEB_TO_MC_QUEUE_URL=https://sqs.ap-northeast-1.amazonaws.com/YOUR_ACCOUNT_ID/kishax-web-to-mc-queue-v2
MC_TO_WEB_QUEUE_URL=https://sqs.ap-northeast-1.amazonaws.com/YOUR_ACCOUNT_ID/kishax-mc-to-web-queue-v2

# API Gateway Configuration
API_GATEWAY_URL=https://YOUR_API_GATEWAY_ID.execute-api.ap-northeast-1.amazonaws.com/prod


# Kishax Application Settings
HOME_SERVER_NAME=spigot
HOME_SERVER_IP=127.0.0.1

# kishax-aws Integration Settings
# if you run local server, uncomment this line to pass the url to docker container
# REDIS_URL=redis://localhost:6379

# if you use production server, uncomment below
# CONFIRM_URL=https://your-confirm-url.com

# AUTH_API_URL=http://localhost:8080 # default: host.docker.internal:8080
AUTH_API_KEY=local-dev-api-key

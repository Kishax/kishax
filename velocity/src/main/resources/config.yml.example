# Kishax Velocity Plugin Configuration

# AWS Configuration for Discord Bot
AWS:
  Region: "ap-northeast-1"
  AccessKey: "YOUR_AWS_ACCESS_KEY_ID"
  SecretKey: "YOUR_AWS_SECRET_ACCESS_KEY"
  ApiGateway:
    URL: "https://your-api-gateway-id.execute-api.ap-northeast-1.amazonaws.com/prod"
    ServiceName: "execute-api"

# Discord Configuration (for reference/migration)
Discord:
  ChannelId: "YOUR_DISCORD_CHANNEL_ID"
  ChatChannelId: "YOUR_DISCORD_CHAT_CHANNEL_ID"
  AdminChannelId: "YOUR_DISCORD_ADMIN_CHANNEL_ID"
  GuildId: "YOUR_DISCORD_GUILD_ID"
  
  # Emoji settings
  BEDefaultEmojiName: "steve"
  
  # Legacy settings (will be deprecated after migration)
  Token: ""
  Webhook_URL: ""

# Database Configuration
Database:
  Host: "localhost"
  Port: 3306
  Name: "kishax"
  User: "kishax_user"
  Password: "your_database_password"

# Server Settings
Servers:
  proxy:
    entry: false
    platform: "velocity"
    memory: 512


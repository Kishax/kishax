# Configuration File for Velocity Plugin (AWS Discord Bot Integration)
EventMessage:
  Join: ""

MySQL:
  Host: "${MYSQL_HOST}"
  Database: "${MYSQL_DATABASE}"
  Port: ${MYSQL_PORT}
  User: "${MYSQL_USER}"
  Password: "${MYSQL_PASSWORD}"

Socket:
  Server_Port: 6000

# AWS Discord Service Configuration
AWS:
  Region: "${AWS_REGION:-ap-northeast-1}"
  ApiGateway:
    Endpoint: "${AWS_API_GATEWAY_ENDPOINT}"
    AccessKey: "${AWS_ACCESS_KEY}"
    SecretKey: "${AWS_SECRET_KEY}"

Debug:
  Mode: false
  Test: false

Permission:
  Detail_Name:
    - something.permission
  Short_Name:
    - someperm

Conv:
  Mode: false
  Host: "localhost"
  EXE_Path: ""

MaxMemory: 0

Servers:
  proxy:
    entry: false
    platform: velocity
    memory: 1
  spigot:
    entry: true
    hub: true
    platform: spigot
    type: null
    modded:
      mode: false
      listUrl: ''
      loaderType: ''
      loaderUrl: ''
    distributed:
      mode: false
      url: ''
    memory: 1
    exec: /mc/server/home/u1.sh
    allow_prestart: true